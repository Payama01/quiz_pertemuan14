// File: /prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model WishlistItem {
  id        String   @id @default(cuid()) 
  nama      String   
  harga     Int       
  createdAt DateTime @default(now())
}